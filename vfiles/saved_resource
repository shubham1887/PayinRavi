    <script type="text/javascript">
        var val1 = "";
        function animation(type) {

            var url = '/Home/pagecontetn'
            $.ajax({
                url: url,
                data: { type: type },
                cache: false,
                type: "POST",
                success: function (data) {
                    if (data != "") {
                        document.getElementById("shoppingBasket").innerHTML = data;
                        $('#defaultModal').modal('show');
                    }
                },
                error: function (reponse) {
                    alert("error : " + reponse);
                }
            });

        }
    </script>
